[alias]
    add-grep = ! ${GIT_ALIASES:-$HOME/.git-aliases}/git-grep-add
    diff-grep = ! ${GIT_ALIASES:-$HOME/.git-aliases}/git-diff-grep
    diff-smart = ! ${GIT_ALIASES:-$HOME/.git-aliases}/git-diff-smart
    set-upstream-to-origin = ! ${GIT_ALIASES:-$HOME/.git-aliases}/git-set-upstream-to-origin
    ag = add-grep
    dg = diff-grep
    ds = diff-smart
    suto = set-upstream-to-origin
