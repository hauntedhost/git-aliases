    diff-grep = ! $GIT_ALIASES/git-diff-grep
    diff-rust = ! $GIT_ALIASES/git-diff-rust
    diff-smart = ! $GIT_ALIASES/git-diff-smart
    set-upstream-to-origin = ! $GIT_ALIASES/git-set-upstream-to-origin
